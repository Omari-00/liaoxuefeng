
def merge(aList,bList):
    if aList == []:
        return bList
    elif  bList == []:
        return aList
    a = aList[0]
    b = bList[0]
    if aList[0] < bList[0]:
        return [a] + merge(aList[1:],bList)
    elif aList[0] > bList[0]:
        return [b] + merge(aList,bList[1:])
aList = [1,3,5,7]
bList = [2,4,6,8]
print(merge(aList,bList))
